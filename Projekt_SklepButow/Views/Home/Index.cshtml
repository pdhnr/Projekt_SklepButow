@{
}

<table class="table">
    <!-- ? -->
    <tbody>
        <!-- ? -->
        @foreach (var eB in Model)
        {

            <tr>
                <!-- tr - Kazdy formularz zapisuje pionowo (Вертикально) -->


                <td>@eB.Id</td> <!-- td - pionowo wypisuje (Вертикально) -->
