                <td>@eB.Model</td>
                <td>@eB.Rodzaj</td>
                <td>@eB.Kolor</td>
                <td>@eB.Cena</td>

                <!-- ------------------------------------------------------------------------------------------------------------------- -->



            </tr>
        }



    </tbody>


    <td>
        <!--  <a class="btn btn-danger" asp-controller="Home" asp-action="Add">Dodaj</a>  -->

        <form asp-controller="Home" asp-action="Add" method="Post" class="btn btn-danger">
            <button type="submit">Dodaj przykladowa ksiazke</button>
        </form>

    </td>

</table>


